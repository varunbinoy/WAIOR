from typing import Callable

class PySolveInterrupter:
    def __init__(self) -> None: ...
    def add_interruption_callback(self, callback: Callable[[], None]) -> int: ...
    def add_trigger_target(self, target: PySolveInterrupter) -> None: ...
    def interrupt(self) -> None: ...
    def remove_interruption_callback(self, callback_id: int) -> None: ...
    def remove_trigger_target(self, target: PySolveInterrupter) -> None: ...
    @property
    def interrupted(self) -> bool: ...
